{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#181818",
    "activityBar.border": "#1f1f1f",
    "activityBarBadge.background": "#228df2",
    "editor.background": "#000000",
    "editor.foreground": "#d6d6dd",
    "editor.inactiveSelectionBackground": "#3636367e",
    "editor.selectionHighlightBackground": "#add6ff26",
    "editorGroupHeader.tabsBackground": "#181818",
    "editorIndentGuide.activeBackground1": "#707070",
    "editorIndentGuide.background1": "#404040",
    "input.placeholderForeground": "#7b7b7b",
    "list.dropBackground": "#383b3d",
    "menu.background": "#181818",
    "menu.foreground": "#d6d6dd",
    "settings.numberInputBackground": "#292929",
    "settings.textInputBackground": "#292929",
    "sideBar.background": "#181818",
    "sideBar.border": "#1f1f1f",
    "sideBarTitle.foreground": "#d1d1d1",
    "statusBar.background": "#181818",
    "tab.activeBackground": "#181818",
    "tab.activeBorder": "#181818",
    "tab.activeBorderTop": "#228df2",
    "tab.border": "#181818",
    "tab.inactiveBackground": "#44444494"
  },
  "tokenColors": [
    {
      "scope": [
        "source.groovy.embedded",
        "meta.template.expression",
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java",
        "meta.definition.variable.name",
        "entity.name.variable",
        "meta.object-literal.key",
        "source",
        "meta.embedded.block.hsx",
        "meta.embedded.block.haskell"
      ],
      "settings": {
        "foreground": "#d6d6dd"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.haskell",
        "punctuation.section.embedded.end.haskell",
        "constant.numeric",
        "markup.inserted",
        "meta.preprocessor.numeric",
        "keyword.other.unit",
        "constant.sha.git-rebase",
        "variable.other.member.haskell",
        "meta.record.definition.haskell"
      ],
      "settings": {
        "foreground": "#ebc88d"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "meta.diff.header",
        "storage",
        "storage.modifier",
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof",
        "variable.language",
        "keyword",
        "modifier"
      ],
      "settings": {
        "foreground": "#83d6c5"
      }
    },
    {
      "scope": [
        "string",
        "markup.deleted",
        "markup.inline.raw",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#e394dc"
      }
    },
    {
      "scope": [
        "support.function",
        "support.constant.handlebars",
        "entity.name.function",
        "storage.type.haskell",
        "meta.attribute",
        "keyword.other.let.haskell",
        "entity.other.attribute-name.multipart.nix"
      ],
      "settings": {
        "foreground": "#aaa0fa"
      }
    },
    {
      "scope": ["constant", "meta.object-literal.key"],
      "settings": {
        "foreground": "#efb080"
      }
    },
    {
      "scope": ["punctuation.definition.tag"],
      "settings": {
        "foreground": "#898989"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.whitespace.comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#6d6d6d",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.parameter",
        "support.function.nix",
        "keyword.operator.bind.nix"
      ],
      "settings": {
        "foreground": "#d6d6dd"
      }
    },
    {
      "scope": [
        "meta.import",
        "entity.name.namespace",
        "support.module",
        "entity.name.type.module"
      ],
      "settings": {
        "foreground": "#87c3ff"
      }
    },
    {
      "scope": [
        "entity.name.type.class",
        "support.class",
        "entity.name.class",
        "entity.other.inherited-class",
        "storage.class",
        "entity.name.tag.html"
      ],
      "settings": {
        "foreground": "#87c3ff"
      }
    },
    {
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#82d2ce"
      }
    },
    {
      "scope": [
        "meta.record.field.haskell",
        "record.name.haskell",
        "entity.name.record.field.haskell",
        "entity.other.attribute-name.haskell",
        "variable.other.field.haskell",
        "entity.other.attribute-name.class.css"
      ],
      "settings": {
        "foreground": "#f8c762"
      }
    },
    {
      "scope": [
        "keyword.operator.assignment.haskell",
        "keyword.operator.eq.haskell",
        "keyword.control.case.haskell",
        "keyword.other.nix"
      ],
      "settings": {
        "foreground": "#83d6c5"
      }
    },
    {
      "scope": [
        "meta.record punctuation.section.braces",
        "meta.record punctuation.brace",
        "punctuation.definition.record.begin.haskell",
        "punctuation.definition.record.end.haskell",
        "punctuation.section.bracket.curly.begin.haskell",
        "punctuation.section.bracket.curly.end.haskell"
      ],
      "settings": {
        "foreground": "#87c3ff"
      }
    },
    {
      "scope": [
        "meta.record.value.haskell",
        "meta.record.field.value.haskell",
        "variable.other.generic-type.haskell"
      ],
      "settings": {
        "foreground": "#d6d6dd"
      }
    }
  ],
  "semanticTokenColors": {
    "selfParameter": "#83d6c5",
    "newOperator": "#83d6c5",
    "stringLiteral": "#e394dc",
    "customLiteral": "#aaa0fa",
    "numberLiteral": "#ebc88d"
  }
}
